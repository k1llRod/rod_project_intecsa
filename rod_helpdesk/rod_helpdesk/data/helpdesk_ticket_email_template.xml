<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">
        <record id="email_template_helpdesk_ticket_with_accessories" model="mail.template">
            <field name="name">Ticket Helpdesk - Accesorios Entregados</field>
            <field name="email_from">${(user.email or '')|safe}</field>
            <field name="subject">Ticket ${object.name} – Resumen de recepción</field>
            <field name="model_id" ref="helpdesk.model_helpdesk_ticket"/>
            <field name="auto_delete" eval="True"/>
            <field name="lang">${object.partner_id.lang}</field>
            <field name="body_html" type="html">
                <div>
                  <strong> ES UNA PRUEBA </strong>
                </div>
            </field>
        </record>

    </data>
</odoo>
