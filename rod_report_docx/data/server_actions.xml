<odoo>
    <data>
        <record id="action_generate_word_report" model="ir.actions.server">
            <field name="name">Nota de entrega (WORD)</field>
            <field name="model_id" ref="sale.model_sale_order"/>
            <field name="binding_model_id" ref="sale.model_sale_order"/>
            <field name="state">code</field>
            <field name="code">
                action = {
                    'type': 'ir.actions.act_url',
                    'url': '/sale_order/report/%s' % record.id,
                    'target': 'self',
                }
            </field>
        </record>
    </data>
</odoo>