<odoo>
    <data>
        <record id="view_warehouse_inh" model="ir.ui.view">
            <field name="name">stock.warehouse.form.inh</field>
            <field name="model">stock.warehouse</field>
            <field name="inherit_id" ref="stock.view_warehouse"/>
            <field name="arch" type="xml">
                <xpath expr="sheet/group/group[2]/field[@name='partner_id']" position="after">
                    <field name="street"/>
                    <field name="street2"/>
                    <field name="city"/>
                    <field name="state_id"/>
                    <field name="zip"/>
                    <field name="country_id"/>
                    <field name="phone"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>