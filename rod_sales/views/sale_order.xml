<odoo>
    <data>
        <record id="sale_order_type_sale" model="ir.ui.view">
            <field name="name">sale.order.form.inh.type.sale</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='country_code']" position="before">
                    <field name="type_sale_id"/>
                </xpath>
            </field>
        </record>
    </data>
    <menuitem id="menu_rod_commission" name="Comisiones" parent="sale.sale_menu_root" sequence="22" />
    <menuitem id="menu_rod_sales_commissions" name="Tipo de comision" parent="menu_rod_commission" action="action_commission_id" sequence="1" />
</odoo>