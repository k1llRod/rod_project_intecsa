<odoo>
    <data>
        <record id="view_account_journal_form_inh_rod" model="ir.ui.view">
            <field name="name">account.journal.form.inh.rod</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
                <xpath expr="sheet/group[1]/group[1]/field[@name='type']" position="after">
                    <field name="title_report"/>
                    <field name="select_report"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>